{
  "name": "webpack-tpl-admin",
  "version": "0.0.1",
  "description": "构建后台模版框架vue2",
  "main": "index.js",
  "scripts": {
    "fix": "eslint ./*.js  --fix"
  },
  "husky": {
      "hooks": {
        "pre-commit": "lint-staged",
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
      }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.less": [
      "stylelint --fix"
    ]
},
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "lint": "^0.7.0",
    "lint-staged": "^11.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-standard": "^22"
  }
}
